{
  "name": "go-todo",
  "private": true,
  "version": "0.0.0",
  "type": "module",
  "scripts": {
    "dev": "run-p watch:* air browser-sync:start",
    "watch:vite": "vite build --watch=true",
    "watch:tsc": "tsc --watch --noEmit",
    "build:vite": "vite build --watch=false",
    "build": "run-s tsc build:vite",
    "tsc": "tsc",
    "air": "air",
    "browser-sync": "browser-sync",
    "browser-sync:start": "browser-sync start --config bs-config.json"
  },
  "devDependencies": {
    "browser-sync": "^3.0.2",
    "npm-run-all": "^4.1.5",
    "typescript": "^5.2.2",
    "vite": "^5.2.0"
  },
  "dependencies": {
    "htmx.org": "^1.9.12"
  }
}
